void noteOn(int cmd, int pitch, int velocity) {
  Serial.write(cmd);
  Serial.write(pitch);
  Serial.write(velocity);
}

void insChange(int cmd,int ins){
  Serial.write(cmd);
  Serial.write(ins);
  }
